@using Cardstone.Web.Models.DeckViewModels
@model DeckViewModel


@{
    Layout = "~/Views/Shared/_SignedInLayout.cshtml";
}

@{
    ViewData["Title"] = "Deck";
}

<h1>My Cards</h1>

<div>
    <div class="grid-container">

        @foreach (var playerCard in Model.PlayersCards)
        {
            <div class="grid-item"><img src="@playerCard.Card.ImageURL" height="200" width="200" "@playerCard.Card.Name" /></div>
        }
    </div>
</div>